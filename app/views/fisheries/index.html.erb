<h1>Listing fisheries</h1>

<table>
  <thead>
     <% @fisheries.each do |fishery| %>
    <tr>
      <th><%= fishery.name %></th>
    </tr>
     <% end %>
  </thead>

  <tbody>
    <% @fisheries.each do |fishery| %>
      <tr>
        <td><%= link_to 'Show', fishery %></td>
        <td><%= link_to 'Edit', edit_fishery_path(fishery) %></td>
        <td><%= link_to 'Destroy', fishery_path(fishery), method: :delete, data: { confirm: "Are you sure you want to delete #{fishery.name}" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fishery', new_fishery_path %>
