<section id="list-a-fishery">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Add a water to <%= @fishery.name %></h2>
                <h3 class="section-subheading text-muted">Fill in some details.</h3>
                <h3 id="notice" class="section-subheading text-muted"><%= notice %></h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
               <%= render "water_form" %>
            </div>
        </div>
    </div>
</section>
