<%= form_for(@fishery, url: "/list/fishery", :html => {:role => "form"}) do |f| %>
  <div class="row">
    <div class="col-md-6">
      <div class="form-group">
        <label for="fishery-name">Fishery Name</label>
        <%= f.text_field :name, class: "form-control", id:"fishery-name", placeholder: "Enter fishery name"%>
      </div>
      <%= render "admin/address", form: f %>
    </div>
    <div class="col-md-6">
      <%= render "admin/contact_details", form: f %>
    </div>
    <div class="clearfix"></div>
    <div class="col-lg-12 text-center">
        <%= button_tag "Submit fishery details", class: "btn btn-xl" %>
    </div>
  </div>
<% end %>
