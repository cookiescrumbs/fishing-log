<ul class="forecast text-muted">
    <li>
        <i class="wi <%=@weather.icon%> fa-2x"></i>
    </li>
    <li>
        <%=@weather.wind_speed%> mph <i class="wi wi-strong-wind"></i>
    </li>
    <li>
    <i class="wi wi-wind from-<%=@weather.wind_bearing%>-deg fa-2x"></i>
        <%=@weather.wind_direction_string%>
    </li>
    <li>
        <%=@weather.fahrenheit %>
        <i class="wi wi-fahrenheit fa-2x"></i>
    </li>
</ul>