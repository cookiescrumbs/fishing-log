.photo-with-text-overlay {
   margin: 0 0 15px 0;
}
.photo-with-text-overlay a {
  text-decoration: none;
}

.photo-with-text-overlay img {
  filter: brightness(90%);
  -webkit-filter: brightness(90%);
}

.photo-with-text-overlay a:hover img {
  text-decoration: none;
}

.photo-with-text-overlay .title-header-outer {
    position: relative;
}

.photo-with-text-overlay .title-header {
    position: absolute;
    width: 100%;
    z-index: 1;
    top: 40px;
}

.photo-with-text-overlay h2 {
    text-transform: none;
    text-align: center;
    color: #fff;
    font-weight: normal;
}

.simple-card {
   margin: 0 0 15px 0;
}

.simple-card a {
  text-decoration: none;
}

.simple-card .caption {
    margin: 0 auto;
    padding: 15px;
    max-width: 650px;
    text-align: center;
    background-color: #fff;
    border-top: 1px solid #f7f7f7;
    color: #333;
}

.simple-card p {
   font-style: italic;
   font-family: "Droid Serif","Helvetica Neue",Helvetica,Arial,sans-serif;
}

.img-responsive {
  width: 100%;
}
